{
    "manifest_version": 3,
    "name": "HabitUP",
    "version": "1.0",
    "description": "A simple habit-tracking extension.",
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "permissions": ["storage"],
    "icons": {
      "128": "icon.png"
    }
  }
  